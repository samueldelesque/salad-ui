{
  "name": "salad-ui-wrapper",
  "version": "0.0.4",
  "description": "",
  "scripts": {
    "dist:copyrequiredcss": "cp ./src/styles/transitions.scss ./dist/salad-ui.transitions/",
    "dist:css:watch": "npm run dist:copyrequiredcss; sass ./src/styles/transitions.scss:./dist/salad-ui.transitions/transitions.css --watch --style compressed",
    "dist:css": "npm run dist:copyrequiredcss; sass ./src/styles/transitions.scss:./dist/salad-ui.transitions/transitions.css --style compressed",
    "distribute": "npm run dist:css; npm run build; webpack --config ./conf/webpack.config.prod.js",
    "dist": "npm run distribute",
    "distribute:demo": "sass ./src/styles/demo.scss -f ./dist/demo/demo.css; sass ./src/styles/transitions.scss:./dist/demo/transitions.css --style compressed; webpack --config ./conf/webpack.config.demo.prod.js",
    "dist:demo": "npm run distribute:demo",
    "lint": "eslint src",
    "publish": "npm run tests; npm run dist; node ./publish.js; git add .; git commit -m 'increment salad-ui version'; git push",
    "pub": "npm run publish",
    "tests": "mocha build/tests/*.js",
    "webpack": "node build/webpack-server.js",
    "dev": "npm run build; concurrently --kill-others 'npm run build-watch' 'npm run dist:css:watch' 'npm run webpack' 'node build/server.js --local'",
    "dev-client": "npm run build; concurrently --kill-others 'npm run build-watch' 'npm run dist:css:watch' 'npm run webpack' 'node build/server.js --is-client --local'",
    "build": "babel src -d build",
    "build-watch": "babel src --watch -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dailymotion/salad-ui.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-chart",
    "react-graph",
    "react-simple-charts",
    "charting"
  ],
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.5",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.30.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.11.1",
    "mocha": "^2.5.3",
    "moment": "^2.12.0",
    "node-sass": "^3.4.2",
    "optimist": "^0.6.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.4.1",
    "sass": "^0.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "clamp-js": "^0.7.0",
    "console-polyfill": "^0.2.2",
    "lodash.merge": "^4.3.5",
    "node-fetch": "^1.5.3",
    "numeral": "^1.5.3",
    "object.entries": "^1.0.3",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.3.2",
    "route-parser": "0.0.5",
    "sprintf-js": "^1.0.3",
    "webpack": "^1.12.14"
  }
}
